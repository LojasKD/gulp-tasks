{
  "name": "@lojaskd/gulp-tasks",
  "version": "1.4.2",
  "description": "Gulp tasks for use in LojasKD projects",
  "author": "LojasKD development team <ti@lojaskd.com.br>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LojasKD/gulp-tasks.git"
  },
  "homepage": "https://github.com/LojasKD/gulp-tasks#readme",
  "bugs": "https://github.com/LojasKD/gulp-tasks/issues",
  "scripts": {
    "lint": "eslint ./**/*.js --quiet",
    "test": "NODE_ENV=test tape tests/*.spec.js | tap-spec",
    "test:coverage": "nyc npm test",
    "publish": "npm publish --access=public",
    "prepublish": "npm test && npm run lint",
    "preversion": "npm test && npm run lint",
    "publish:pa": "npm version patch && npm publish",
    "publish:mi": "npm version minor && npm publish",
    "publish:ma": "npm version major && npm publish"
  },
  "keywords": [
    "gulp",
    "build",
    "tasks"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "nyc": {
    "functions": 100,
    "lines": 100,
    "check-coverage": true,
    "exclude": "tests/**",
    "extension": [
      ".js",
      ".es6"
    ],
    "reporter": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "gulp-autoprefixer": "^3.1.1",
    "gulp-batch-replace": "^0.0.0",
    "gulp-combine-mq": "^0.4.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-gzip": "^1.4.0",
    "gulp-header": "^1.8.9",
    "gulp-include": "^2.3.1",
    "gulp-jsbeautifier": "^2.0.4",
    "gulp-load-plugins": "^1.4.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.0.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "lodash": "^4.17.4",
    "node-bourbon": "^4.2.8",
    "require-dir": "^0.3.1"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-config-lojaskd-base": "^0.0.2",
    "gulp": "^3.9.1",
    "husky": "^0.14.3",
    "mkdirp": "^0.5.1",
    "nyc": "^11.2.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
